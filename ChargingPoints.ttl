@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix dbo: <http://dbpedia.org/> .
@prefix mv: <http://schema.mobivoc.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix s: <http://schema.org/> .
@prefix gr: <http://purl.org/goodrelations/v1#> .
@prefix vcard: <http://www.w3.org/2001/vcard-rdf/3.0#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

mv:Charger
    a rdfs:Class, owl:Class ;
    rdfs:comment "Describes characteristics of the charger." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Charger"@en, "Ladegerät"@de .

mv:ChargingPoint
    a rdfs:Class, owl:Class ;
    rdfs:comment "Defines charging points for electric vehicles." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Charging point"@en, "Ladestation"@de ;
    rdfs:subClassOf s:CivicStructure .

mv:Plug
    a rdfs:Class, owl:Class ;
    rdfs:comment "Describes plug characteristics." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Anschluss"@de, "Plug"@en .

mv:plugType
    a rdf:Property, owl:ObjectProperty ;
    rdfs:comment "Relates the plug to its plug type." ;
    rdfs:domain mv:Plug ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "plug type"@en ;
    rdfs:range mv:PlugType .

mv:PlugType
    a rdfs:Class, owl:Class ;
    rdfs:comment "Describes the type of available plug." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Plug type"@en, "Steckertyp"@de .

mv:SchukoPlug
    rdfs:comment "Plug with Schuko plug connection." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Schuko plug"@en ;
    a mv:PlugType ;
    owl:sameAs dbpedia:Schuko ;
    foaf:depiction <http://upload.wikimedia.org/wikipedia/commons/e/ef/Schuko_plug_and_socket.png> .

mv:Type1Plug
    rdfs:comment "Plug with Type 1 plug connection." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Type 1 plug"@en ;
    a mv:PlugType ;
    foaf:depiction <http://commons.wikimedia.org/wiki/Special:FilePath/SAE_J1772_7058855567.jpg> .

mv:Type2Plug
    rdfs:comment "Plug with Type 2 plug connection." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Type 2 plug"@en ;
    a mv:PlugType ;
    owl:sameAs "http://en.wikipedia.org/wiki/IEC_62196#Type_2:_VDE-AR-E_2623-2-2" ;
    foaf:depiction <http://upload.wikimedia.org/wikipedia/commons/f/f3/VDE-AR-E_2623-2-2-plug.jpg> .

mv:Type3Plug
    rdfs:comment "Plug with Typ 3 plug connection." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Type 3 plug"@en ;
    a mv:PlugType ;
    skos:altLabel "EV plug" .

mv:CHAdeMOPlug
    rdfs:comment "Plug with CHAdeMO connection." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "CHAdeMO"@en ;
    a mv:PlugType ;
    owl:sameAs dbpedia:CHAdeMO ;
    foaf:depiction <http://upload.wikimedia.org/wikipedia/commons/f/f3/VDE-AR-E_2623-2-2-plug.jpg> .

mv:ComboPlug
    rdfs:comment "Plug with combo connection." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "Combo Plug"@en ;
    a mv:PlugType .

mv:EUDomesticPlug ### check plug types
    rdfs:comment "Plug with EU domestic plug connection." ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "EU Domestic Plug"@en ;
    a mv:PlugType ;
    owl:sameAs dbpedia:Europlug .

mv:brand
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Indicates the charger brand." ;
    rdfs:domain mv:Charger ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "brand"@en , "Marke"@de;
    rdfs:range xsd:string .

mv:cableAvailable
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Indicates whether the plug has a cable." ;
    rdfs:domain mv:Plug ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "cable available"@en , "Kabel verfügbar"@de ;
    rdfs:range xsd:boolean .

mv:energySource
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Indicates the energy source of the charger point i.e. renewable etc." ;
    rdfs:domain mv:ChargerPoint ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "energy source"@en ;
    rdfs:range xsd:string .

mv:charger ### better definition!
    a rdf:Property, owl:ObjectProperty ;
    rdfs:comment "Charging point charger." ;
    rdfs:domain mv:ChargingPoint ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "charger"@en ;
    rdfs:range mv:Charger .

mv:plug
    a rdf:Property, owl:ObjectProperty ;
    rdfs:comment "Plugs available at the charging point." ;
    rdfs:domain mv:Charger ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "plug"@en ;
    rdfs:range mv:Plug .

mv:model
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Charger model of the charging point." ;
    rdfs:domain mv:Charger ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "model"@en , "Modell"@de;
    rdfs:range xsd:string .

mv:threePhasedCurrentAvailable ###still needed and relevant?
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Indicates whether three-phased current is available." ;
    rdfs:domain [
          a owl:Class ;
          owl:unionOf (mv:Plug mv:Charger)
        ];
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "three-phased current available"@en ;
    rdfs:range xsd:boolean .

mv:voltageInV
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Indicates the voltage of a charger or plug in volt." ;
    rdfs:domain [
          a owl:Class ;
          owl:unionOf (mv:Plug mv:Charger)
        ];
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "voltage in V"@en ;
    rdfs:range xsd:decimal .

mv:currentInA
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Indicates the current of a charger or plug in ampere." ;
    rdfs:domain [
          a owl:Class ;
          owl:unionOf (mv:Plug mv:Charger)
        ];
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "current in A"@en ;
    rdfs:range xsd:decimal .

mv:powerInKW
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Indicates the power of a charger or plug in kilowatts." ;
    rdfs:domain mv:Plug ;
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "power in kW"@en ;
    rdfs:range xsd:decimal .

mv:currentType
    a rdf:Property, owl:DatatypeProperty ;
    rdfs:comment "Indicates the current type (AC/DC) of the plug." ;
    rdfs:domain [
          a owl:Class ;
          owl:unionOf (mv:Plug mv:Charger)
        ];
    rdfs:isDefinedBy <http://schema.mobivoc.org/> ;
    rdfs:label "current type"@en ;
    rdfs:range xsd:string . 
