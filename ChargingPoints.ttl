# CLASSES

mv:AccessInformation
	a rdfs:Class, owl:Class ;
	rdfs:comment	"Access information of the charging point." ;
	rdfs:label "Access Information"@en, "Zugangsinformation"@de ;
	rdfs:isDefinedBy mv: .

mv:AccessType
	a rdfs:Class, owl:Class ;
	rdfs:comment	"Describes access type of the charging point." ;
	rdfs:label "Access Type"@en ;
	rdfs:isDefinedBy mv: .

mv:Charger
	a rdfs:Class, owl:Class ;
	rdfs:comment	"Describes characteristics of the charger." ;
	rdfs:label "Charger"@en, "Ladegerät"@de ;
	rdfs:isDefinedBy mv: .

mv:ChargingPoint
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf s:CivicStructure ;
	rdfs:comment	"Defines the public or semi-public charging points for electric vehicles available worldwide. " ;
	rdfs:label "Charging Point"@en , "Ladestation"@de ;
	rdfs:isDefinedBy mv: .

mv:ChargingPointFees
	a rdfs:Class, owl:Class ;
	rdfs:comment	"Captures information about the charging point fees." ;
	rdfs:label "Charging Point Fees"@en ;
	rdfs:isDefinedBy mv: .

mv:ChargingPointOwner
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf	gr:BusinessEntity ;
	rdfs:comment	"Captures information about the owner of charging point." ;
	rdfs:label "Charging Point Owner"@en , "Besitzer der Ladestation"@de ;
	rdfs:isDefinedBy mv: .

mv:ChargingSpeed
	a rdfs:Class, owl:Class ;
	rdfs:comment	"Describes speed types available at the types of vehicle that can be loaded by charging point." ;
	rdfs:label "Charging Speed"@en ;
	rdfs:isDefinedBy mv: .

mv:IdentificationSystem
	a rdfs:Class, owl:Class ;
	rdfs:comment	"Describes identification system of the charging point." ;
	rdfs:label "Identification System"@en ;
	rdfs:isDefinedBy mv: .

mv:Plug
	a rdfs:Class, owl:Class ;
	rdfs:comment	"Describes  plug characteristics." ;
	rdfs:label "Plug"@en, "Anschluss"@de ;
	rdfs:isDefinedBy mv: .

mv:hasPlugType
a	rdf:Property, owl:ObjectProperty ;
	rdfs:label	"has plug type"@en ;
	rdfs:comment	"Relates the plug to its plugtype" ;
	rdfs:domain	mv:Plug ;
	rdfs:range	mv:PlugType  ;
	rdfs:isDefinedBy mv: .

mv:PlugType
	a rdfs:Class, owl:Class ;
	rdfs:comment	"Describes the type of available plug." ;
	rdfs:label "Plug Type"@en, "Steckertyp"@de ;
	rdfs:isDefinedBy mv: .

mv:ChargeColumn
	a owl:Class ;
	rdfs:comment "Describes a charge column" ;
	rdfs:label "Ladesäule"@de ,"Charge column"@en ;
	rdfs:isDefinedBy mv: .

# INSTANCES

# Plug Types

mv:ComboPlug
	a owl:Class ;
	rdfs:subClassOf mv:PlugType ;
	rdfs:comment "Plug with combo connection." ;
	rdfs:label "Combo Plug"@en ;
	rdfs:isDefinedBy mv: .

mv:CHAdeMOPlug
	a owl:Class ;
	rdfs:subClassOf mv:PlugType ;
	rdfs:label "CHAdeMO"@en ;
	rdfs:comment "Plug with CHAdeMO connection." ;	
	owl:sameAs <http://dbpedia.org/resource/CHAdeMO> ;
	foaf:depiction <http://upload.wikimedia.org/wikipedia/commons/f/f3/VDE-AR-E_2623-2-2-plug.jpg> ;
	rdfs:isDefinedBy mv: .

mv:EUDomesticPlug
	a owl:Class ;
	rdfs:subClassOf mv:PlugType ;
	owl:sameAs <http://dbpedia.org/resource/Europlug> ;
	rdfs:label "EU Domestic Plug"@en ;
	rdfs:comment "Plug with EU domestic plug connection." 
	rdfs:isDefinedBy mv: .

mv:SchukoPlug
	rdfs:subClassOf mv:PlugType ;
	owl:sameAs <http://dbpedia.org/resource/Schuko> ;
	foaf:depiction <http://upload.wikimedia.org/wikipedia/commons/e/ef/Schuko_plug_and_socket.png> ;
	rdfs:label "Schuko Plug"@en ;
	rdfs:comment "Plug with Schuko plug connection." ;
	rdfs:isDefinedBy mv: .

mv:Type1Plug
	a owl:Class ;
	rdfs:subClassOf mv:PlugType ;
	rdfs:label "Type1 Plug"@en ;
	rdfs:comment "Plug with Type 1 plug connection." ;
	owl:sameAs <http://dbpedia.org/resource/SAE_J1772> ;
	foaf:depiction <http://commons.wikimedia.org/wiki/Special:FilePath/SAE_J1772_7058855567.jpg> ;
	rdfs:isDefinedBy mv: .

mv:Type2Plug
	a owl:Class ;
	rdfs:subClassOf mv:PlugType ;
	rdfs:label "Type2 Plug"@en ;
	rdfs:comment "Plug with Type 2 plug connection." ;
	owl:sameAs "http://en.wikipedia.org/wiki/IEC_62196#Type_2:_VDE-AR-E_2623-2-2" ;
	foaf:depiction <http://upload.wikimedia.org/wikipedia/commons/f/f3/VDE-AR-E_2623-2-2-plug.jpg> ;
	rdfs:isDefinedBy mv: .

mv:Type3Plug
	a owl:Class ;
	rdfs:subClassOf mv:PlugType ;
	rdfs:label "Type3 Plug"@en ;
	rdfs:comment "Plug with Typ 3 plug connection." ;
	skos:altLabel "EV Plug" ;
	rdfs:isDefinedBy mv: .

mv:UnknownPlug
	a owl:Class ;
	rdfs:subClassOf mv:PlugType ;
	rdfs:label "Unknown Plug"@en , "Unbekannter Anschluss"@de ;
	rdfs:comment "Plug with unknown connection." ;
	rdfs:isDefinedBy mv: .

