# PROPERTIES

# Abstract Properties

mv:access
a	rdf:Property, owl:ObjectProperty ;
	rdfs:label	"access"@en ;
	rdfs:comment	"Indicates the access types of charging point. " ;
	rdfs:domain	mv:AccessInformation ;
	rdfs:range	mv:AccessType  ;
	rdfs:isDefinedBy mv: .

mv:accessible
a	rdf:Property, owl:ObjectProperty ;
	rdfs:label	"is accessible"@en ;
	rdfs:comment	"Indicates the access information of the charging point." ;
	rdfs:domain	mv:ChargingPoint ;
	rdfs:range	mv:AccessInformation ;
	rdfs:isDefinedBy mv: .

mv:accessTimes
a	rdf:Property, owl:DatatypeProperty ;
	rdfs:label	"Access Time"@en ;
	rdfs:comment	"Defines access time of the charging point. " ;
	rdfs:domain	mv:AccessInformation ;
	rdfs:range	xsd:string 
	rdfs:isDefinedBy mv: .

mv:availableFromTime
a	rdf:Property, owl:DatatypeProperty ;
	rdfs:label	"available From Time"@en ;
	rdfs:comment	"Information when the charging point will be available." ;
