	rdfs:label "Video surveillance"@en , "Videoüberwachung"@de ;
	rdfs:isDefinedBy mv: .

mv:SurveillanceBySecurityGuards
	a mv:ParkingFacilityFeature ;
	rdfs:comment "The parking facility is surveilled by security guards."@en ;
