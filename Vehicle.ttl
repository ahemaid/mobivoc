@prefix mv: <http://schema.mobivoc.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

# VEHICLE

mv:Bicycle
	rdfs:subClassOf mv:Vehicle ;
	rdfs:label "Bicycle"@en, "Fahrrad"@de ;
	rdfs:comment "A a human-or electric powered, pedal-driven, single-track vehicle, having two wheels attached to a frame, one behind the other."@en ;
	rdfs:isDefinedBy mv: .

mv:Motorcycle
	rdfs:subClassOf mv:Vehicle ;
	rdfs:label "Motorcycle"@en, "Motorrad"@de ;
	rdfs:comment "A two- or three-wheeled motor vehicle."@en ;
	rdfs:isDefinedBy mv: .

mv:Car
	rdfs:subClassOf mv:Vehicle ;
	rdfs:label "Car"@en, "Auto"@de ;
	skos:altLabel "Pkw"@de ;
	rdfs:comment "A wheeled, self-powered motor vehicle used for transportation."@en ;
	rdfs:isDefinedBy mv: .

mv:RecreationalVehicle
	rdfs:subClassOf mv:Vehicle ;
	rdfs:label "Recreational vehicle"@en, "Wohnmobil"@de ;
	rdfs:comment "A motor vehicle or trailer equipped with living space and amenities found in a home."@en ;
	rdfs:isDefinedBy mv: .
